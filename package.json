{
  "name": "-api",
  "version": "1.0.0",
  "description": "Backend API for Assignment",
  "main": "server.js",
  "author": "Binil Baby.",
  "license": "All Rights Reserved",
  "engines": {
    "node": ">=14.5.0",
    "vscode": "^1.41.1"
  },
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=.env node -r dotenv/config ./build/server.js",
    "start:debug": "DEBUG=express:* DOTENV_CONFIG_PATH=.env node -r dotenv/config ./build/server.js",
    "start:inspect": "DOTENV_CONFIG_PATH=.env node -r dotenv/config --inspect ./build/server.js",
    "dev": "DOTENV_CONFIG_PATH=.env ts-node -r dotenv/config ./src/server.ts",
    "dev:watch": "nodemon --config nodemon.json",
    "clean": "rimraf ./build",
    "build": "npm run lint && npm run clean && tsc",
    "build:watch": "tsc -w",
    "lint": "tslint --project .",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "lint-staged",
    "precommit": "npm run prettier && npm run lint",
    "prepush": "npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --parser typescript --write",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/express": "^4.17.3",
    "@types/uuid": "^3.4.8",
    "cors": "^2.8.5",
    "dd-trace": "^0.20.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "fs": "0.0.1-security",
    "i18next": "^19.3.2",
    "i18next-express-middleware": "^1.9.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jest": "^24.0.25",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
}
